<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- QWeb Reports -->

        <record id="business_application_acknowledging_letter" model="ir.actions.report">
            <field name="name">Print Acknowledging template Letter</field>
            <field name="model">business.application</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">business_lending_application.template_acknowledging_letter</field>
            <field name="report_file">business_lending_application.template_acknowledging_letter</field>
            <field name="binding_model_id" ref="model_business_application"/>
