        </record>

        <record id="business_application_accounting_detail" model="ir.actions.report">
            <field name="name">Print Business Accounting Detail of Application</field>
            <field name="model">business.application</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">business_lending_application.template_business_account</field>
            <field name="report_file">business_lending_application.template_business_account</field>
            <field name="binding_model_id" ref="model_business_application"/>
            <field name="binding_type">report</field>
        </record>

    </data>
</odoo>